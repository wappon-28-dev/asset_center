{
  "extends": [
    "@tsconfig/strictest/tsconfig.json",
    "./.svelte-kit/tsconfig.json"
  ],
  "compilerOptions": {
    "types": ["gtag.js"],
    "paths": {
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"],
      "*": ["../api/src/*", "./src/*"],
      "@api": ["../api/src/index.ts"],
      "@api/types/*": ["../api/src/lib/types/*"]
    }
  },
  "exclude": ["../api/src/**"]
}
